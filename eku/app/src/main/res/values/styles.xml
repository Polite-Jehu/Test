<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="title">

        <item name="android:textSize">
            16sp
        </item>

        <item name="android:letterSpacing">
            -0.01
        </item>

        <item name="android:textColor">
            #252525
        </item>

    </style>

    <style name="userInfo">

        <item name="android:textSize">
            14sp
        </item>

        <item name="android:letterSpacing">
            0.04
        </item>

        <item name="android:textColor">
            #80252525
        </item>

    </style>

    <style name="subTitle">
        <item name="android:textSize">
            11sp
        </item>

        <item name="android:letterSpacing">
            -0.01
        </item>

        <item name="android:textColor">
            #80252525
        </item>
    </style>

    <style name="signInButton">
        <item name="paddingStart">
            16dp
        </item>
        <item name="android:paddingTop">
            4dp
        </item>
        <item name="android:paddingBottom">
            4dp
        </item>
        <item name="android:textSize">
            13sp
        </item>
    </style>

    <style name="font_size_11">
        <item name="android:textSize">
            11sp
        </item>
    </style>

    <style name="font_size_13">
        <item name="android:textSize">
            13sp
        </item>
    </style>

    <style name="font_size_16">
        <item name="android:textSize">
            16sp
        </item>
    </style>

    <style name="Widget.Theme.AppCompat.Light.NoActionBar.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>
</resources>